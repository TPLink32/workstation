#include <stdio.h>
#include <stdlib.h>

int add(int x, int y)
{
        return x + y;
}
typedef struct function{
        int x;
        int y;
        int (*add)(x, y);
}fun;

int main()
{
        fun function;
        int x = 1;
        int y = 2;
        int z = function.add(x, y);
        printf("%d\n", z);

        return 0;
}

